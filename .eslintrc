{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "semi": "off",
    "object-curly-spacing": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "new-cap": "off",
    "no-trailing-spaces": "off",
    "require-jsdoc": "off",
    "quotes": "off",
    "padded-blocks": "off",
    "eol-last": "off",
    "max-len": "off",    
    "no-unused-vars": "off",
    "indent": "off"
  },
  "plugins": ["jest"],
  "extends": ["eslint:recommended", "google"]  
}